<UserControl x:Class="FluGASv25.Views.ReferenceAView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/xaml/behaviors"
        xmlns:l="http://schemas.livet-mvvm.net/2011/wpf"
        xmlns:local="clr-namespace:FluGASv25.Views"
        mc:Ignorable="d">
    <UserControl.Resources>
        <ResourceDictionary Source="Styles.xaml" />
    </UserControl.Resources>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="50" />
            <RowDefinition Height="*" />
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="3"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="3"/>
        </Grid.ColumnDefinitions>

        <TextBlock Grid.Row="0" Grid.Column="1" Background="#eae7e7" 
            HorizontalAlignment="Stretch" VerticalAlignment="Top">
            　【Reference update】<LineBreak/>
            　リファレンス配列の更新を行います。<LineBreak/>
            　<LineBreak/>
        </TextBlock>
        <ScrollViewer Grid.Row="1" Grid.Column="1" >
            <Grid Background="#eae7e7" Margin="0,5,0,0">
                <Grid.RowDefinitions>
                    <RowDefinition Height="2*" />
                    <RowDefinition Height="2*"/>
                    <RowDefinition Height="7*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="5"/>
                    <ColumnDefinition Width="5*"/>
                    <ColumnDefinition Width="5"/>
                </Grid.ColumnDefinitions>

                <!-- NCBI reference -->
                <GroupBox Grid.Column="1" Grid.Row="0" Background="#f9f8eb"
                FontSize="20"
                Header="NCBI reference">
                    <StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <Label />
                            <TextBox x:Name="DownloadUrl"                    
                                 VerticalAlignment="Center" HorizontalAlignment="Stretch"
                                 FontSize="12" 
                                 Text="{Binding DownloadUrl , Mode=OneTime}"
                                 IsReadOnly="True"/>
                            <Label/>
                        </StackPanel>
                    </StackPanel>
                </GroupBox>

                <!-- user reference -->
                <GroupBox Grid.Column="1" Grid.Row="1" Background="#f9f8eb"
                    FontSize="20"
                    Header="User reference fasta">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="5*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>

                        <TextBox Grid.Column="0" Grid.Row="0"
                            x:Name="UserFasta"                    
                            Height="18"
                            VerticalAlignment="Center" HorizontalAlignment="Stretch" Margin="10,0,5,0"
                            FontSize="12"      
                            Text="{Binding UserFasta}" />

                        <Button Grid.Column="1" Grid.Row="0"
                            HorizontalAlignment="Left" VerticalAlignment="Center" Margin="15,0,20,0"
                            FontSize="12"
                            Style="{StaticResource FlatButton}"
                            Content="Select"  
                            Command="{Binding SelectUserFastaCommand, UpdateSourceTrigger=PropertyChanged}" />

                    </Grid>
                </GroupBox>

